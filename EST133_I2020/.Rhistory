quanti<-function(q,li,ls,fi){
#q<-0.5
r<-sum(fi)*q
i<-1;z<-1
Fx<-cumsum(fi)
while(z==1){
if(r<Fx[i]){
z<-0
} else {
i<-i+1
}
}
if(i>1){
Q<-(li[i]+((ls[i]-li[i])*(r-Fx[i-1])/(fi[i])))
} else {
Q<-(li[i]+((ls[i]-li[i])*(r-0)/(fi[i])))
}
return(Q)
}
